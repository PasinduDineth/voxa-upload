# TikTok OAuth Configuration
# These need to be set in Vercel environment variables

# Server-side only (NEVER expose in frontend)
TIKTOK_CLIENT_KEY=your_tiktok_client_key_here
TIKTOK_CLIENT_SECRET=your_tiktok_client_secret_here
TIKTOK_REDIRECT_URI=https://www.pasindu.website/callback

# Frontend environment variables (exposed to client)
REACT_APP_TIKTOK_CLIENT_KEY=your_tiktok_client_key_here
REACT_APP_REDIRECT_URI=https://www.pasindu.website/callback

# Vercel Postgres (auto-configured by Vercel)
POSTGRES_URL=your_postgres_url_here
POSTGRES_PRISMA_URL=your_postgres_prisma_url_here
POSTGRES_URL_NON_POOLING=your_postgres_url_non_pooling_here
POSTGRES_USER=your_postgres_user_here
POSTGRES_HOST=your_postgres_host_here
POSTGRES_PASSWORD=your_postgres_password_here
POSTGRES_DATABASE=your_postgres_database_here
